generator client {
  provider = "prisma-client"
  output   = "./generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Question {
  id         String   @id @default(uuid())
  question   String
  answer     String
  createAt   DateTime @default(now())
  updatedAt  DateTime @default(now())
  isReviewed Boolean  @default(false)
}
